			function illustrate(jobStatistics){
				d3.select("svg").remove();
				var barHeight = 10;
				var barGap = 10;
				var topAndBottomPadding = 0;
				var sidePadding = 300;
				var axisHeight = 20;
				var width = 1200;
				var slotConfig = calculateSlots(jobStatistics);
				var height = calculateHeight(slotConfig);
				var toolBarHeight = 22;
				var svg = d3.select("#canvas")
					.append("svg")
					.attr("class", "svg")
					.attr("width", width)
					.attr("height", height);
				var duration = calculateDuration(jobStatistics);
				console.log("duration=" + duration);
				var timeScale = d3.scale.linear()
					.domain([0, duration])
					.range([0, width - sidePadding]);
				makeAxis(svg, timeScale, slotConfig);
				drawTasks(jobStatistics, slotConfig, svg, timeScale);

				function calculateSlots(jobStatistics){
					var nodeUsage = collectNodeUsage(jobStatistics);
					var colorScale = d3.scale.linear()
							.domain([0, Object.keys(nodeUsage).length])
							.range(["#00B9FA", "#F95002"])
							.interpolate(d3.interpolateHcl);
					calculateSlotPlacement(nodeUsage, colorScale);
					return nodeUsage;
					/*
					[
						maps: {
							node_id_map_0:{
								jobs: {
									job1: {},
									job2: {}
								}
							}
						},
						reduces: {
							
						}
					]
					*/
					function collectNodeUsage(jobStatistics){
						var maps = {}
						var reduces = {};
						for(var i in jobStatistics){
							var job = jobStatistics[i];
							var jobID = job.jobID;
							var nodes = job.nodes;
							var mapSlots = job.mapSlots;
							var reduceSlots = job.reduceSlots;
							for(var j in nodes){
								var nodeID = nodes[j];
								insertNodeRecord(maps, nodeID, jobID, mapSlots, "MAP");
								insertNodeRecord(reduces, nodeID, jobID, reduceSlots, "REDUCE");
							}
						}
						var usage = joinIntoOneObject(maps, reduces);
						return usage;

						function insertNodeRecord(nodeUsage, nodeID, jobID, slotCount, slotType){
							for(var slotIndex = 0; slotIndex < slotCount; slotIndex++){
								var slotName = nodeID + "_" + slotType + "_" + slotIndex;
								if(!(slotName in nodeUsage)){
									nodeUsage[slotName] = {"jobs": {}};
								}
								nodeUsage[slotName].jobs[jobID] = {};
							}
						}

						function joinIntoOneObject(obj1, obj2){
							var result = {};
							for(var key in obj1){
								result[key] = obj1[key];
							}
							for(var key in obj2){
								result[key] = obj2[key];
							}
							return result;
						}
					}

					function calculateSlotPlacement(nodeUsage, colorScale){
						var startY = 0;
						var slotIndex = 0;
						for(var slotKey in nodeUsage){
							var slot = nodeUsage[slotKey];
							var jobs = slot.jobs;
							var jobCount = Object.keys(jobs).length;
							slot.startY = startY;
							slot.height = (jobCount + 1) * (barHeight + barGap) + barGap;
							slot.color = colorScale(slotIndex);
							startY += (barGap + barHeight + barGap);
							for(var jobID in jobs){
								jobs[jobID].startY = startY;
								startY += (barGap + barHeight);
							}
							slotIndex++;
						}
					}
				}

				function calculateHeight(slotConfig){
					var keys = Object.keys(slotConfig);
					var lastKey = keys[keys.length - 1];
					var lastSlot = slotConfig[lastKey];
					var height = topAndBottomPadding + lastSlot.startY + lastSlot.height + topAndBottomPadding + axisHeight;
					return height;
				}

				function calculateDuration(jobStatistics){
					return d3.max(jobStatistics, function(job){return job.durationInSecond;});
				}

				function makeAxis(svg, timeScale, slotConfig){
					drawXAxis(svg, timeScale);
					drawYAxis(svg, slotConfig);
				}

				function drawXAxis(svg, timeScale){
					var xAxis = d3.svg.axis()
						.scale(timeScale)
						.orient('bottom')
						.ticks(10)
						.tickSize(-height + topAndBottomPadding + axisHeight, 0)
						.tickFormat(function(t){
							return t + "s";
						});

					var grid = svg.append('g')
						.attr('class', 'grid')
						.attr('transform', 'translate(' +sidePadding + ', ' + (height - axisHeight - topAndBottomPadding) + ')')
						.call(xAxis)
						.selectAll("text")  
						.style("text-anchor", "middle")
						.attr("fill", "#000")
						.attr("stroke", "none")
						.attr("font-size", 10)
						.attr("dy", "1em");
				}

				function drawYAxis(svg, slotConfig){
					var rows = collectRows(slotConfig);
					var axisText = svg.append("g")
						.selectAll("text")
						.data(rows)
						.enter()
						.append("text")
						.text(function(d){
							return d.text;
						})
						.attr("x", 10)
						.attr("y", function(d){
							return d.y;
						})
						.attr("height", barHeight)
						.attr("font-size", 10)
						.attr("text-anchor", "start");

					function collectRows(slotConfig){
						var result = [];
						for(var slotKey in slotConfig){
							var slot = slotConfig[slotKey];
							var header = {y: topAndBottomPadding + barGap + slot.startY + 10, text: slotKey};
							result.push(header);
							var jobs = slot.jobs;
							for(var jobID in jobs){
								var job = jobs[jobID];
								var jobRow = {y: topAndBottomPadding + job.startY + 10, text: jobID};
								result.push(jobRow);
							}
						}
						return result;
					}
				}

				function drawTasks(jobStatistics, slotConfig, svg, timeScale){
					drawSlots(slotConfig, svg);
					drawTaskGantt(jobStatistics, slotConfig, svg, timeScale);

					function drawSlots(slotConfig, svg){
						var slotRects = svg.append("g")
							.selectAll("rect")
							.data(Object.keys(slotConfig))
							.enter()
							.append("rect")
							.attr("x", 0)
							.attr("y", function(s){
								return slotConfig[s].startY;
							})
							.attr("width", width)
							.attr("height", function(s){
								return slotConfig[s].height;
							})
							.attr("stroke", "black")
							.attr("fill", function(s){
								return d3.rgb(slotConfig[s].color);
							})
							.attr("opacity", 0.2);
					}

					function drawTaskGantt(jobStatistics, slotConfig, svg, timeScale){
						var tasks = collectTasks(jobStatistics, slotConfig);
						console.log(tasks);
						drawTaskRects(tasks, svg, timeScale);

						function collectTasks(jobStatistics, slotConfig){
							var collectedTasks = [];
							for(var i in jobStatistics){
								var job = jobStatistics[i];
								var jobID = job.jobID;
								var jobTasks = job.tasks;
								for(var j in jobTasks){
									var task = jobTasks[j];
									var slotName = createSlotName(task);
									var slotInformation = (slotConfig[slotName].jobs)[jobID];
									collectedTasks.push({"task": task, "slot" : slotInformation});
								}
							}
							return collectedTasks;

							function createSlotName(task){
								return task.node + "_" + task.type + "_" + task.slot;
							}
						}

						function drawTaskRects(tasks, svg, timeScale){
							var taskRects = svg.append("g")
							.selectAll("rect")
							.data(tasks)
							.enter()
							.append("rect")
							.attr("x", function(t){
								var x = timeScale(t.task.startTimeInSecond) + sidePadding;
								console.log("x=" + x);
								return x;
							})
							.attr("y", function(t){
								var y = t.slot.startY + topAndBottomPadding;
								console.log("y=" + y);
								return y
							})
							.attr("rx", 5)
							.attr("ry", 5)
							.attr("height", barHeight)
							.attr("width", function(t){
								var width = timeScale(t.task.endTimeInSecond) - timeScale(t.task.startTimeInSecond);
								console.log("width=" + width);
								return width;
							})
							.attr("stroke", "none")
							.attr("class", function(t){
								console.log(t.task.type);
								if(t.task.type == "MAP"){
									return "mapperBar";
								}else{
									return "reducerBar";
								}
							});

							taskRects.on("mouseover", function(e){
								var t = d3.select(this).data()[0];
								var tag = "";
								for(var key in t.task){
									tag += (key + ": " + t.task[key] + "<br/>");
								}
								var output = document.getElementById("tag");
								var x = (this.x.animVal.value + this.width.animVal.value/2 - 300 / 2 + 80) + "px";
								var y = this.y.animVal.value + barHeight + topAndBottomPadding + toolBarHeight + 15 + "px";
								output.innerHTML = tag;
								output.style.top = y;
								output.style.left = x;
								output.style.display = "block";
							})
							.on("mouseout", hideTaskDetail);

							function hideTaskDetail(){
								var output = document.getElementById("tag");
								output.style.display = "none";
							}
						}
					}
				}
			}

		</script>
